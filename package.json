{
  "name": "products-management",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "prepare": "cd .. && husky install products-management/.husky",
    "pre-commit-lint": "pnpm lint-staged",
    "pre-push-lint": "rm -rf storybook-static/ && build-storybook -s public && vite build"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "jest": "^28.0.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^28.1.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.8.1",
    "react-testing-library": "^8.0.1",
    "storybook": "^7.0.6",
    "ts-jest": "^28.0.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern",
      "pnpm run test"
    ],
    "src/**/*.{css}": [
      "npx prettier --write"
    ]
  }
}
